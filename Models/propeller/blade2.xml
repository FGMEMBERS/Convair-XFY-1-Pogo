<?xml version="1.0"?>

<PropertyList>

  <model>
    <name>Blade1</name>
    <path>Aircraft/Convair-XFY-1-Pogo/Models/propeller/blade.xml</path>
  </model>

  <!-- Incidence de la pale -->
  <animation>
    <type>rotate</type>
    <object-name>blade</object-name>
    <property>rotors/main/blade[1]/incidence-deg</property>
    <center>
       <x-m> 0.000 </x-m>
       <y-m> 0.000 </y-m>
       <z-m> 0.000 </z-m>
    </center>
    <axis>
      <x>  0.000 </x>
      <y> -1.000 </y>
      <z>  0.000 </z>
    </axis>
  </animation>

</PropertyList>

